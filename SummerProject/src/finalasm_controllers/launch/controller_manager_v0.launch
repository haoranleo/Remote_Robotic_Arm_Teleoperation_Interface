<!-- -*- mode: XML -*- -->

<launch>
  <node name="finalasm_controller_manager_1" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
    <rosparam>
        namespace: finalasm_manager
        serial_ports:
            port_0:
                port_name: "/dev/ttyUSB0"
                baud_rate: 1000000
                min_motor_id: 1
                max_motor_id: 4
                update_rate: 200
            port_1:
                port_name: "/dev/ttyUSB1"
                baud_rate: 1000000
                min_motor_id: 5
                max_motor_id: 10
                update_rate: 200
    </rosparam>
  </node>
</launch>
